CREATE TABLE EMPLOYEE(
   NAME VARCHAR (30) NOT NULL,
   ID INT NOT NULL,
   AGE INT NOT NULL,
   ADDRESS CHAR (40) ,
   SALARY   DECIMAL (18, 2),       
   PRIMARY KEY (ID)
);
INSERT INTO EMPLOYEE VALUES("Athul",7,22,"Jayanagar",200000);
INSERT INTO EMPLOYEE VALUES("Abhi",8,22,"Banashankari",200000);

CREATE TABLE CUSTOMER (
 CUSTOMER_ID INT references CUSTOMERS(ID),
   ID INT NOT NULL,
   AMOUNT double,
   PRIMARY KEY (ID)
);

INSERT INTO CUSTOMER VALUES(7,2000);
INSERT INTO CUSTOMER VALUES(8,3000);


SELECT * FROM EMPLOYEE;

SELECT * FROM EMPLOYEE LEFT JOIN SALARY ON aID = AMOUNT;

